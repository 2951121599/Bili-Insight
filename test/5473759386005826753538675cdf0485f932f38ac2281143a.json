      "from": 18.67,
      "to": 19.21,
      "sid": 10,
      "location": 2,
      "content": "开幕式"
    },
    {
      "from": 19.21,
      "to": 20.74,
      "sid": 11,
      "location": 2,
      "content": "那里布置5g基站"
    },
    {
      "from": 20.84,
